# API Configuration
API_URL=http://localhost:3000/api/v1
WS_URL=ws://localhost:3000

# App Configuration
APP_NAME=Fintech
APP_DISPLAY_NAME=Fintech Platform
APP_IDENTIFIER=com.fintech.app

# Environment
ENV=development

# External Services - OneSignal (Push Notifications)
ONESIGNAL_APP_ID=your_onesignal_app_id

# External Services - Google Services
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GOOGLE_SERVICES_ANDROID=./google-services.json
GOOGLE_SERVICES_IOS=./GoogleService-Info.plist

# External Services - Sentry
SENTRY_DSN=

# Features
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_BIOMETRIC_AUTH=true
ENABLE_ANALYTICS=true

# Storage Keys
STORAGE_AUTH_TOKEN=@fintech:auth_token
STORAGE_REFRESH_TOKEN=@fintech:refresh_token
STORAGE_USER_DATA=@fintech:user_data
STORAGE_SETTINGS=@fintech:settings

# Card Display
MASK_CARD_NUMBER=true
SHOW_CVV_BY_DEFAULT=false

# Security
ENABLE_SSL_PINNING=true
TIMEOUT_MS=30000

# Deep Linking
DEEP_LINK_SCHEME=fintech
DEEP_LINK_PREFIX=https://app.fintech-platform.com

# Version
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Code Push (for OTA updates)
CODEPUSH_DEPLOYMENT_KEY_IOS=
CODEPUSH_DEPLOYMENT_KEY_ANDROID=
