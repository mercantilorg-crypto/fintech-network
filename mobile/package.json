{
  "name": "fintech-mobile",
  "version": "1.0.0",
  "description": "Mobile application for Fintech Platform",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:ios": "detox test -c ios.sim.debug",
    "test:e2e:android": "detox test -c android.emu.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "react-native clean",
    "reset": "rm -rf node_modules && rm -rf ios/Pods && npm install && cd ios && pod install",
    "build:ios:dev": "react-native bundle --platform ios --dev true --entry-file index.js --bundle-output ios/main.jsbundle",
    "build:ios:prod": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:aab": "cd android && ./gradlew bundleRelease",
    "deploy:ios:testflight": "fastlane ios beta",
    "deploy:android:beta": "fastlane android beta"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.7",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "axios": "^1.6.2",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-keychain": "^8.1.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-onesignal": "^5.0.2",
    "react-native-config": "^1.5.1",
    "react-native-svg": "^14.0.0",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.42",
    "@types/react-test-renderer": "^18.0.7",
    "typescript": "^5.3.2",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.1",
    "react-test-renderer": "^18.2.0",
    "detox": "^20.14.0",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
